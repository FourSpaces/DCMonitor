{
    "influxdb": {
        "influxdbUrl": "http://192.168.10.60:8086",
        "influxdbDatabase": "dcmonitor",
        "influxdbUser": "root",
        "influxdbPassword": "root"
    },
    "zookeeper": {
        "addrs": "192.168.10.60:2181,192.168.10.41:2181,192.168.10.42:2181",
        "connectionTimeout": 5000,
        "baseSleepTimeMs": 1000,
        "maxRetries": 20,
        "maxSleepMs": 30000
    },
    "druid": {
        "zkInfoCompress": false,
        "zkRootPath": "/druid/prod",
        "zkDiscoveryPath": "/prod/discovery",
        "zkOverlordName": "druid:prod:overlord",
        "zkBrokerName": "druid:prod:broker",
        "zkCoordinatorName": "druid:prod:coordinator"
    },
    "kafka": {
        "warning": true,
        "warnDefaultLag": 100,
        "warnLagSpec": {
            "test|dsp_druid_ingester_0": 200
        },
        "@comment": "set ignoreConsumerRegex to ignore sending warning on those test consumers",
        "ignoreConsumerRegex": "^console-consumer-.+$",
        "stormKafkaRoot": "/clicki_track_storm"
    },
    "notify": {
        "doSend": false,
        "appName": "myApp",
        "url": "http://notify.com",
        "emails": ["test@test.com"],
        "phones": []
    },
    "fetchers": {
        "druidFetcher": {
            "fetchPeriod": "PT5S"
        },
        "kafkaFetcher": {
            "fetchPeriod": "PT5S"
        },
        "zookeeperFetcher": {
            "fetchPeriod": "PT5S"
        }
    }
}